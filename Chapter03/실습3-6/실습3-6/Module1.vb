Module Module1

    Sub Main()
        Console.WriteLine("*****************")
        Console.WriteLine("20 << 2 ==> {0} ", (20 << 2))
        Console.WriteLine("20 >> 2 ==> {0} ", (20 >> 2))
        Console.WriteLine("*****************")

        Console.WriteLine("-20 << 2 ==> {0} ", (-20 << 2))
        Console.WriteLine("-20 >> 2 ==> {0} ", (-20 >> 2))

        Console.WriteLine("*****************")
        Console.ReadLine()
    End Sub

End Module