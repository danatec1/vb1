Module Module1

    Sub Main()
        Console.WriteLine("4 ＋ 4 * 5 / 2 － 10 ＝ {0}" _
                   , (4 + 4 * 5 / 2 - 10))
        Console.WriteLine("(4 ＋ 4) * 5 / 2 － 10 ＝{0}" _
                             , ((4 + 4) * 5 / 2 - 10))
        Console.WriteLine("4 ＋ 3 Mod 2 * 2 ＝ {0}" _
                 , (4 + 3 Mod 2 * 2))
        Console.WriteLine("4 ＋ 3 * 2 Mod 2 ＝ {0}" _
                 , (4 + 3 * 2 Mod 2))
        Console.ReadLine()
    End Sub

End Module
